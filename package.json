{
  "name": "game-of-life-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:jcarlosmejiaayala/game-of-life-challenge.git",
  "author": "jcarlosmejiaayala <jcarlosmejiaayala@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack",
    "dev": "webpack-dev-server --open --progress",
    "pretest": "yarn lint",
    "test": "jest",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "prepush": "yarn test"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.3",
    "styled-normalize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.6",
    "babel-loader": "8.0.0-beta.4",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "raf": "^3.4.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add --force"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      "components(.*)$": "<rootDir>/src/components/$1",
      "routes(.*)$": "<rootDir>/src/routes/$1"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/*.spec.js?(x)"
    ],
    "setupFiles": [
      "<rootDir>/testing.setup.js"
    ]
  }
}
